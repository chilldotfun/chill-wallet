{
  "name": "Chill",
  "description": "A Memecoin Trading Wallet",
  "version": "1.0.8",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "sidePanel", "tabs", "windows", "webNavigation"],

  "action": {
    "default_icon": {
      "16": "/images/logo16.png",
      "32": "/images/logo32.png",
      "48": "/images/logo48.png",
      "128": "/images/logo128.png"
    }
  },
  "icons": {
    "16": "/images/logo16.png",
    "32": "/images/logo32.png",
    "48": "/images/logo48.png",
    "128": "/images/logo128.png"
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "content_scripts": [],
  "web_accessible_resources": [
    {
      "resources": ["js/component.js", "popup.html", "images/insertToScript/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
